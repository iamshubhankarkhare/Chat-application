  "license": "MIT",
  "dependencies": {
    "@webscopeio/react-textarea-autocomplete": "^4.6.3",
    "cors": "^2.8.5",
    "emoji-mart": "^3.0.0",
    "express": "^4.17.1",
    "react-feather": "^2.0.8",
    "socket.io": "^2.4.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
